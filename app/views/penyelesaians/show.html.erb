<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Form penyelesaian</h2>
    <ol class="breadcrumb">
      <li>
        <a href="">Home</a>
      </li>
      <li>
        <a href="<%= penyelesaians_url %>">List</a>
      </li>
      <li class="active">
        <strong>Detail</strong>
      </li>
    </ol>
  </div>
</div>
<br>
<div id="content">
  <div class="page-header">
    <%= link_to penyelesaians_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
    <% end %>
    <%= link_to edit_penyelesaian_path(@penyelesaian), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
    <% end %>
<!--    <h1>Show </h1>-->
  </div>

  <dl class="dl-horizontal">
      <dt>Kode penyelesaian:</dt>
      <dd><%= @penyelesaian.kode_penyelesaian %></dd>

      <dt>Tgl selesai:</dt>
      <dd><%= @penyelesaian.tgl_selesai %></dd>

      <dt>Durasi:</dt>
      <dd><%= @penyelesaian.durasi %></dd>

      <dt>Pic:</dt>
      <dd><%= @penyelesaian.pic %></dd>

  </dl>
</div>
