<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Form uat</h2>
    <ol class="breadcrumb">
      <li>
        <a href="">Home</a>
      </li>
      <li>
        <a href="<%= uats_url %>">List</a>
      </li>
      <li class="active">
        <strong>Form</strong>
      </li>
    </ol>
  </div>
</div>
<br>
<div id="content">
  <div class="page-header">
    <%= link_to new_uat_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Uat
    <% end %>
    <!--    <h1>Listing </h1>-->
  </div>

  <div class="table-responsive">
    <table class="table table-striped table-bordered table-hover dataTables-example">
      <thead>
      <tr>
        <th>No</th>
                  <th>No testing</th>
                  <th>Tgl testing</th>
                  <th>Penguji</th>
                <th></th>
        <th></th>
        <th></th>
      </tr>
      </thead>

      <tbody>
      <% @uats.each_with_index do |uat, index| %>
      <tr>
        <td><%= index+1 %></td>
                  <td><%= uat.no_testing %></td>
                  <td><%= uat.tgl_testing %></td>
                  <td><%= uat.penguji %></td>
                <td><%= link_to 'Show', uat %></td>
        <td><%= link_to 'Edit', edit_uat_path(uat) %></td>
        <td><%= link_to 'Destroy', uat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
