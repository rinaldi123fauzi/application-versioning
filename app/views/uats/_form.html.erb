<div class="panel panel-default">
  <div class="panel-heading">Form uat</div>
  <div class="panel-body">
<%= form_for(@uat, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @uat.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@uat.errors.count, "error") %> prohibited this uat from being saved:</h4>

      <ul>
      <% @uat.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.hidden_field :id_rfc, value: params[:id] %>
  <div class="form-group">
    <%= f.label :no_testing, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <% if @uat.new_record? %>
        <%= f.text_field :no_testing, value: @no_uat, class: "form-control", readonly: true %>
      <% else %>
        <%= f.text_field :no_testing, value: @uat.no_testing, class: "form-control", readonly: true %>
      <% end %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :tgl_testing, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.date_field :tgl_testing, class: "form-control", required: true %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :penguji, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :penguji, class: "form-control", required: true %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit "Pending Release", class: "btn btn-primary" %>
      <input type="submit" value="> Next to Release" class="btn btn-primary" name="next_line">
    </div>
  </div>
<% end %>
  </div>
</div>